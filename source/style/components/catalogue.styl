.catalogue
	
	position fixed
	width 100%
	height 100%
	-webkit-perspective 1100
	-webkit-perspective-origin 50%
	overflow scroll

	&--item
		background cover
		position absolute
		background-size cover
		height 160px
		width 160px

		&--inner
			height 60px
			position absolute
			bottom -60px
			



